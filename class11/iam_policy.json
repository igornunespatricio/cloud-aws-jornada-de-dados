{
	"Version": "2012-10-17",
	"Statement": [
		{
			"Effect": "Allow",
			"Action": "logs:CreateLogGroup",
			"Resource": "arn:aws:logs:us-east-2:ACCOUNT_ID:*"
		},
		{
			"Effect": "Allow",
			"Action": [
				"logs:CreateLogStream",
				"logs:PutLogEvents"
			],
			"Resource": [
				"arn:aws:logs:us-east-2:ACCOUNT_ID:log-group:/aws/lambda/url-tutorial:*"
			]
		},
		{
			"Effect": "Allow",
			"Action": "s3:*",
			"Resource": [
				"arn:aws:s3:::bucket-to-test-123254",
				"arn:aws:s3:::bucket-to-test-123254/*"
			]
		}
	]
}